{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "runtime": "nodejs12"
  },
  "redirects": [
    {
    "source": "/login",
    "type": 302,
    "destination" : "/login.html"
  },
  {
  "source": "/register",
  "type": 302,
  "destination" : "/register.html"
}]
}
